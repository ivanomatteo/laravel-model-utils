{
    "name": "ivanomatteo/model-utils",
    "description": "Extract info from models, generate laravel factories, generate phpdoc comment with property name for type hinting",
    "keywords": [
        "ivanomatteo",
        "laravel",
        "model-utils"
    ],
    "homepage": "https://github.com/ivanomatteo/model-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivano Matteo",
            "email": "ivanomatteo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "IvanoMatteo\\ModelUtils\\": "src",
            "IvanoMatteo\\ModelUtils\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IvanoMatteo\\ModelUtils\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IvanoMatteo\\ModelUtils\\ModelUtilsServiceProvider"
            ],
            "aliases": {
                "ModelUtils": "IvanoMatteo\\ModelUtils\\Facades\\ModelUtils"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
